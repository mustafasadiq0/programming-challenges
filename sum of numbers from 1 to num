// the user will enter a number and the result is tha sum from 1 to number. Example enter 4, the result is 1+2+3+4=10.


#include <iostream>
using namespace std;

int SimpleAdding(int num) { 

  int i,sum=0;
  for (i=1;i<=num;i++)
  sum=sum+i;
  
  return sum; 
            
}

int main() { 
  
  // keep this function call here
  cout << SimpleAdding(gets(stdin));
  return 0;
    
}
