#include <iostream>
#include<string>

using namespace std;

string FirstReverse(string str) { 
    string str1=str;
  int i;
  for (i=0;i<str.length();i++)
  str1[i]=str[str.length()-i-1];
  
  return str1; 
            
}

int main() { 
  
  // keep this function call here
  cout << FirstReverse(gets(stdin));
  return 0;
    
}
